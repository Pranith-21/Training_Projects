//sum of elemnts in sub array
#include<stdio.h>
int main()
{
    int a[80],i,n,b;
    printf("enter the number of values in the array");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    b=max_array(a,n);
    printf("%d",b);
    
}
int max_array(int *a,int n)
{
    int min=0,max,i,end=0 ,start=0,beg;
    for(i=0;i<n;i++)
    {
        max=a[i]+max;
        if(min<=max)
        {
            min=max;
            start=beg;
            end=i;
            
        }
        if(max<0)
        {
            max=0;
            beg=i+1;
        }
    }
    for(i=start;i<=end;i++)
    {
        printf("%d",a[i]);
    }
    return min;
}


//missing elements in array
#include <stdio.h>
#include<math.h>

int main()
{
   int n,Sum=0,sumSq=0,i;
   int A,B;
  int a[50];
	printf("enter the size");
	scanf("%d",&n);
	printf("enter the elements");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
    int sum=n*(n+1)/2;
    int sumsq=n*(n+1)*(2*n+1)/6;
      for(i=0;i<n;i++)
      {
          Sum+=a[i];
          sumSq = sumSq + (pow(a[i], 2)); 
      }
  
        B = (((sumSq - sumsq) / (Sum - sum)) + sum - Sum) / 2; 
         A = Sum - sum + B; 
      
      printf("%d%d",A,B);
    
    

    return 0;
}

//repetitive positive integers
#include<stdio.h>
#include<math.h>

int main()
{
    int n, sum=0,A,a[70],i;
	printf("enter the size of the array");
	scanf("%d",&n);
	printf("enetr the elements ");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
    int sum1=(n*(n+1))/2;
    for(i=0;i<n;i++)
    {
        sum+=a[i];
    }
    A=sum1-sum;
    printf("%d",A);
    return 0;
    
    
}
